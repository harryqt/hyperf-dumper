{
    "name": "fangx/dumper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "dumper"
    ],
    "description": "",
    "authors": [
        {
            "name": "nfangxu",
            "email": "nfangxu@gmail.com"
        },
        {
            "name": "harry",
            "email": "contact@harry.plus"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "hyperf/http-message": "^2.1 | ^3.1",
        "psr/http-message": "^2.0",
        "symfony/var-dumper": "^6.0 | ^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Fangx\\Dumper\\": "src/"
        },
        "files": [
            "src/func.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Fangx\\Dumper\\ConfigProvider"
        }
    }
}